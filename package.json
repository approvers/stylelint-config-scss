{
  "name": "@approvers/stylelint-config-scss",
  "version": "0.0.0",
  "description": "The shareable SCSS config for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "scss",
    "approvers"
  ],
  "homepage": "https://github.com/approvers/stylelint-config-scss#readme",
  "bugs": {
    "url": "https://github.com/approvers/stylelint-config-scss/issues"
  },
  "license": "MIT",
  "author": "SHUN/しゅん <shun.shobon@gmail.com> (https://github.com/shun-shobon)",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/approvers/stylelint-config-scss.git"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore '**/*.js'",
    "lint:fix": "eslint --ignore-path .gitignore '**/*.js' --fix"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "stylelint-config-standard": "^20.0.0"
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2"
  },
  "peerDependencies": {
    "stylelint": "^10.1.0 || ^11.0.0 || ^12.0.0 || ^13.0.0",
    "stylelint-scss": "^3.0.0"
  }
}
